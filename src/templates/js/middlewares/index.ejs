/*
    Object for storing all references to middleware
*/

<% if(database) { -%>
import setupDatabase from './database.middleware.js';
<% } -%>
<% if (moreOptions && compressionSupport && webFramework === 'express') { -%>
import setupCompression from './compression.middleware.js';
<% } -%>
import setupSecurityHeaders from './security.middleware.js';
import setupJsonParser from './json.middleware.js';
import setupUrlEncodedParser from './urlEncoded.middleware.js';
import setupRequestID from './requestID.middleware.js';
import setupRouters from './routers.middleware.js';
import setupNotFound from './notFound.middleware.js';
import setupError from './error.middleware.js';

// Object containing references to middleware functions used in the application
const middlewares = {
<% if(database) { -%>
    setupDatabase,
<% } -%>
<% if (moreOptions && compressionSupport && webFramework === 'express') { -%>
    setupCompression,
<% } -%>
    setupSecurityHeaders,
    setupJsonParser,
    setupUrlEncodedParser,
    setupRequestID,
    setupRouters,
    setupNotFound,
    setupError,
};

export default middlewares;
